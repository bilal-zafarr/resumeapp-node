<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Resume Generator</title>
  <link rel="icon" href="/img/cv.png">
  <link rel="stylesheet" href="/css/stylesResume.css">
	<script src="https://kit.fontawesome.com/b99e675b6e.js"></script>
</head>
<body>

<div class="resume_wrapper" contenteditable="true">
	<div class="resume_left">
		<div class="resume_image">
			<img src="img/pic.png" alt="Resume_image" id="dp">
			<div class="middle">
    	<button class="text" contenteditable="false">Upload Image</button>
  		</div>
		</div>
		<div class="resume_bottom">
			{{!-- <div class="resume_item resume_namerole">
				<div class="name">{{name}}</div>
				<div class="role">{{role}}</div>
			</div> --}}
			<div class="resume_item resume_profile">
				<div class="resume_title">Profile</div>
				<div class="resume_info" id="info">{{info}}</div>
			</div>
			<div class="resume_item resume_address">
				<div class="resume_title">Address</div>
				<div class="resume_info" id="address">
					{{address}}
				</div>
			</div>
			<div class="resume_item resume_contact">
				<div class="resume_title">Contact</div>
				<div class="resume_info">
					<div class="resume_subtitle">Phone</div>
					<div class="resume_subinfo" id="phone">{{phone}}</div>
				</div>
				<div class="resume_info">
					<div class="resume_subtitle">Email</div>
					<div class="resume_subinfo" id="email">{{email}}</div>
				</div>
			</div>
			<div class="resume_item resume_skills">
				<div class="resume_title">Skills</div>
				<div class="resume_info">
					<div class="skills_list">
						<div class="skills_left" id="skills">{{skills}}</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="resume_right">
		<div class="resume_item resume_namerole">
			<div class="name" id="name">{{name}}</div>
			<div class="role" id="role">{{role}}</div>
		</div>
		<div class="resume_item resume_education">
			<div class="resume_title">Education</div>
			<div class="resume_info">
				<div class="resume_data">
					<div class="year" id="year1">{{year1}}</div>
					<div class="content">
						<p id="title1">{{title1}}</p>
						<p id="desc1">{{desc1}}</p>
					</div>
				</div>
				<div class="resume_data">
					<div class="year" id="year2">{{year2}}</div>
					<div class="content">
						<p id="title2">{{title2}}</p>
						<p id="desc2">{{desc2}}</p>
					</div>
				</div>
				<div class="resume_data">
					<div class="year" id="year3">{{year3}}</div>
					<div class="content">
						<p id="title3">{{title3}}</p>
						<p id="desc3">{{desc3}}</p>
					</div>
				</div>
			</div>
		</div>
		<div class="resume_item resume_experience">
			<div class="resume_title">Experience</div>
			<div class="resume_info">
				<div class="resume_data">
					<div class="year" id="year1ex">{{year1ex}}</div>
					<div class="content">
						<p id="title1ex">{{title1ex}}</p>
						<p id="desc1ex">{{desc1ex}}</p>
					</div>
				</div>
				<div class="resume_data">
					<div class="year" id="year2ex">{{year2ex}}</div>
					<div class="content">
						<p id="title2ex">{{title2ex}}</p>
						<p id="desc2ex">{{desc2ex}}</p>
					</div>
				</div>
				<div class="resume_data">
					<div class="year" id="year3ex">{{year3ex}}</div>
					<div class="content">
						<p id="title3ex">{{title3ex}}</p>
						<p id="desc3ex">{{desc3ex}}</p>
					</div>
				</div>
				<div class="resume_data">
					<div class="year" id="year4ex">{{year4ex}}</div>
					<div class="content">
						<p id="title4ex">{{title4ex}}</p>
						<p id="desc4ex">{{desc4ex}}</p>
					</div>
				</div>
				<div class="resume_data">
					<div class="year" id="year5ex">{{year5ex}}</div>
					<div class="content">
						<p id="title5ex">{{title5ex}}</p>
						<p id="desc5ex">{{desc5ex}}</p>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="buttons">
		<button id="save">Save</button>
		<button id="generatepdf">Generate PDF</button>
</div>
<script src="/js/scriptResume.js"></script>
</body>
</html>
